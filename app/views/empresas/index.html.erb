<p id="notice"><%= notice %></p>

<h1>Empresas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Rua</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Pais</th>
      <th>Cep</th>
      <th>Cnpj</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.nome %></td>
        <td><%= empresa.rua %></td>
        <td><%= empresa.cidade %></td>
        <td><%= empresa.estado %></td>
        <td><%= empresa.pais %></td>
        <td><%= empresa.cep %></td>
        <td><%= empresa.cnpj %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
